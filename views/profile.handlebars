<h1 id="welcome-back">Welcome back, <span id="profile-username">{{name}}</span></h1>
<h2 id="dev">My Devs:</h2>
{{#each Devs}}
<div class="card dev-card">
    <h3>Name: <span id="dev-card-name">{{name}}</span></h3>
    <p>ID: {{id}}</p>
    <p>Level: {{level}}</p>
    <p>Current Experience Points: {{current_exp}}</p>
    <p>Alignment: {{alignment}}</p>
    <p>Health Points: {{hp}}</p>
    <p>Attack: {{attack}}</p>
    <p>Speed: {{speed}}</p>
    <button class="delBtn" data-devId="{{id}}">Delete Me</button>
</div>
<h3>{{name}}'s Movelist:</h3>
<div id="moves-container">
    {{#each Moves}}
    <div class="card">
        <p>Move Name: <span class='highlight'>{{name}}</span></p>
        <p>Power: {{power}}</p>
        <p>Accuracy: {{accuracy}}</p>
    </div>
    {{/each}}
</div>
<hr>
{{/each}}

<script src="/js/profile.js"></script>